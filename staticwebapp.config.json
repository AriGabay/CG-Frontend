{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,js,ico,svg,jpg,gif,png}",
      "assets/*"
    ]
  },
  "routes": [
    {
      "route": "/admin",
      "allowedRoles": [
        "administrator"
      ]
    },
    {
      "route": "/api/*",
      "methods": [
        "PUT",
        "POST",
        "PATCH",
        "DELETE",
        "GET"
      ]
    }
  ]
}